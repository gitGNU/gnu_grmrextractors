input:
     | newlines
{
    drule("input");

    assert($$parser_data->{STACK}->size() == 1);
    $$parser_data->{STACK}->pop();
    assert($$parser_data->{STACK}->is_empty());
}
      | stmt_list
{
    drule("input");

    assert($$parser_data->{STACK}->size() == 1);

    if (!add_window_widget($$parser_data->{WIDGET},
			   \$$parser_data->{WIDGET_ID},
			   \$$parser_data->{STACK})) {
	$_[0]->YYAbort;
    }

    print $parser_data->{WIDGET} . " >>>\n";
}
     | newlines stmt_list
{
    drule("input");

    assert($$parser_data->{STACK}->size() == 1);

    if (!add_window_widget($$parser_data->{WIDGET},
			   \$$parser_data->{WIDGET_ID},
			   \$$parser_data->{STACK})) {
	$_[0]->YYAbort;
    }
}
